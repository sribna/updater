{
    "name": "sribna/updater",
    "description": "Framework agnostic self-updater",
    "homepage": "https://github.com/sribna/updater",
    "require": {
        "php": "^7.0",
        "ext-json": "*",
        "ext-zip": "*",
        "ext-fileinfo": "*",
        "guzzlehttp/guzzle": "~6|~7",
        "netresearch/jsonmapper": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Sribna\\Updater\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sribna\\Updater\\Tests\\": "tests"
        }
    }
}
